Print If == True True   then "PASS: True == True"   else "FAIL: True == True";
Print If == False False then "PASS: False == False" else "FAIL: False == False";
Print If == True False  then "Fail: True == False"  else "PASS: True == False";
Print If == False True  then "Fail: False == True"  else "PASS: False == True";


Print If And True True   then "PASS: True AND True"  else "FAIL: True AND True";
Print If And True False  then "FAIL: True AND False" else "PASS: True AND False";
Print If And False True  then "FAIL: False AND True" else "PASS: False AND True";
Print If And False False then "FAIL: False AND False" else "PASS: False AND False";

Print If Or True True    then "PASS: True OR True" else "FAIL: True OR True";
Print If Or True False  then "PASS: True OR False" else "FAIL: True OR False";
Print If Or False True  then "PASS: False OR True" else "FAIL: False OR True";
Print If Or False False then "FAIL: False OR False" else "PASS: False OR False";

Print If Xor True True   then "FAIL: True XOR True" else "PASS: True XOR True";
Print If Xor True False  then "PASS: True XOR False" else "FAIL: True XOR False";
Print If Xor False True  then "PASS: False XOR True" else "FAIL: False XOR True";
Print If Xor False False then "FAIL: False XOR False" else "PASS: False XOR False";

Print If Not True  then "FAIL: NOT True"  else "PASS: NOT True";
Print If Not False then "PASS: NOT False" else "PASS: NOT False";
